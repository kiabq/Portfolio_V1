---
import Layout from "./Layout.astro"
import Header from "../components/header.astro";
import Footer from "../components/footer.astro";

const posts = await Astro.glob("../pages/posts/*.md");
---

<Layout>
    <ul>
        {posts.map((post) => {
            <li>
                <a href={post.url}>{post.frontmatter.title}</a>
            </li>
        })}
    </ul>
</Layout>